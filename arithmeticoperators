import math


print(1+1)
print(1.0 +1)
print(2.0 +3.0)
#print(1 + 'sameer')  TypeError: unsupported operand type(s) for +: 'int' and 'str'


x = 2+3*4/5

print(x)

y = 2**3
print(y) 


a = 3.8  # upper bound 
b= 3.4   # lower bound
print(round(a))
print(round(b))

print(abs(a))
print(abs(b))

print ('----- ceil and floor value-------------')
print(math.ceil(a))
print(math.floor(a))

print(math.factorial(5))